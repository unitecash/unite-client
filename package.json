{
  "name": "Unite",
  "version": "1.0.0",
  "description": "Unite.cash - We Stand Against Censorship",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-mac": "electron-packager . Unite --overwrite --platform=darwin --arch=x64 --icon=public/images/icon.icns --prune=true --out=build/Release",
    "build-win": "electron-packager . Unite --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/images/icon.ico --prune=true --out=build/Release --version-string.CompanyName=Unite --version-string.FileDescription=Unite --version-string.ProductName=\"Unite\"",
    "build-linux": "electron-packager . Unite --overwrite --asar=true --platform=linux --arch=x64 --icon=public/images/icon.png --prune=true --out=build/Release"
  },
  "repository": "",
  "keywords": [
    "unite",
    "bitcoin",
    "cash",
    "censorship",
    "distributed"
  ],
  "author": "The Unite.cash Developers",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.8.4"
  },
  "dependencies": {
    "electron-packager": "^12.0.1",
    "webpack": "^4.8.2"
  }
}
