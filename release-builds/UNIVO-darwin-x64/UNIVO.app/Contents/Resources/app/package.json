{
  "name": "UNIVO",
  "version": "1.0.0",
  "description": "Universal Internet Voice - We Stand Against Censorship",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
	"package-mac": "electron-packager . UNIVO --overwrite --platform=darwin --arch=x64 --icon=webroot/res/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . UNIVO --overwrite --asar=true --platform=win32 --arch=ia32 --icon=webroot/res/icon.ico --prune=true --out=release-builds --version-string.CompanyName=UNIVO --version-string.FileDescription=UNIVO --version-string.ProductName=\"UNIVO\"",    
    "package-linux": "electron-packager . UNIVO --overwrite --asar=true --platform=linux --arch=x64 --icon=webroot/res/icon.png --prune=true --out=release-builds"
  },
  "repository": "",
  "keywords": [
    "UNIVO",
    "Bitcoin",
    "Cash",
    "Censorship"
  ],
  "author": "UNIVO",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.8.4"
  },
  "dependencies": {
    "electron-packager": "^12.0.1"
  }
}
